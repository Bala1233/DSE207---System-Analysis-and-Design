    <script src="~/js/_customer_side_cart.js"></script>
    <script>
        $(function () {
            $.ajax({
                type: "GET",
                url: "/Admin/CheckRole",
                success: function (data) {
                    if (data == "Not_Admin") {
                        location.href = "/CustomerStore/MerchandisePage";
                    }
                    else if (data == "Null") {
                        location.href = "/CustomerLogin/LoginPage"
                    }
                    else {
                        $("body").css("display", "")
                    }

                }
            })
        })
    </script>
    @await RenderSectionAsync("Scripts", required: false)


